<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xmlns:owl="http://www.w3.org/2002/07/owl#" xml:base="http://www.example.org/hospital_theatre_scheduling.owl"
  xmlns="http://www.example.org/hospital_theatre_scheduling.owl#">

  <!-- Ontology Declaration -->

  <owl:Ontology rdf:about="http://www.example.org/hospital_theatre_scheduling.owl"/>

  <!-- Object Properties (Relationships between entities) -->

  <owl:ObjectProperty rdf:about="#hasPatient">
    <rdfs:domain rdf:resource="#Schedule"/>
    <rdfs:range rdf:resource="#Patient"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasSurgeon">
    <rdfs:domain rdf:resource="#Schedule"/>
    <rdfs:range rdf:resource="#Surgeon"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasOperation">
    <rdfs:domain rdf:resource="#Schedule"/>
    <rdfs:range rdf:resource="#Operation"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasTheatre">
    <rdfs:domain rdf:resource="#Schedule"/>
    <rdfs:range rdf:resource="#Theatre"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#hasTimeSlot">
    <rdfs:domain rdf:resource="#Schedule"/>
    <rdfs:range rdf:resource="#TimeSlot"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#surgeonAvailableAt">
    <rdfs:domain rdf:resource="#Surgeon"/>
    <rdfs:range rdf:resource="#TimeSlot"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#theatreAvailableAt">
    <rdfs:domain rdf:resource="#Theatre"/>
    <rdfs:range rdf:resource="#TimeSlot"/>
  </owl:ObjectProperty>

  <!-- Datatype Properties (Attributes of entities)      -->

  <owl:ObjectProperty rdf:about="#conflictsWith">
    <rdfs:domain rdf:resource="#TimeSlot"/>
    <rdfs:range rdf:resource="#TimeSlot"/>
  </owl:ObjectProperty>

  <owl:DatatypeProperty rdf:about="#hasName">
    <rdfs:domain rdf:resource="#Person"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>


  <owl:DatatypeProperty rdf:about="#isClean">
    <rdfs:domain rdf:resource="#Theatre"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="#isUnderMaintenance">
    <rdfs:domain rdf:resource="#Theatre"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="#isPresent">
    <rdfs:domain rdf:resource="#Surgeon"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="#operationDuration">
    <rdfs:domain rdf:resource="#Operation"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="#priorityLevel">
    <rdfs:domain rdf:resource="#Operation"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="#maxDailyHours">
    <rdfs:domain rdf:resource="#Surgeon"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  </owl:DatatypeProperty>

  <!-- Class Definitions (Core domain concepts)          -->

  <owl:Class rdf:about="#Person">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </owl:Class>

  <owl:Class rdf:about="#Patient">
    <rdfs:subClassOf rdf:resource="#Person"/>
  </owl:Class>

  <owl:Class rdf:about="#Surgeon">
    <rdfs:subClassOf rdf:resource="#Person"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#surgeonAvailableAt"/>
        <owl:someValuesFrom rdf:resource="#TimeSlot"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#isPresent"/>
        <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:hasValue>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Operation">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </owl:Class>

  <owl:Class rdf:about="#Theatre">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#theatreAvailableAt"/>
        <owl:someValuesFrom rdf:resource="#TimeSlot"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class>
        <owl:complementOf>
          <owl:Restriction>
            <owl:onProperty rdf:resource="#isUnderMaintenance"/>
            <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:hasValue>
          </owl:Restriction>
        </owl:complementOf>
      </owl:Class>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#isClean"/>
        <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:hasValue>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#TimeSlot">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </owl:Class>

  <owl:Class rdf:about="#Schedule">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#hasPatient"/>
        <owl:onClass rdf:resource="#Patient"/>
        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#hasSurgeon"/>
        <owl:onClass rdf:resource="#Surgeon"/>
        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#hasOperation"/>
        <owl:onClass rdf:resource="#Operation"/>
        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#hasTheatre"/>
        <owl:onClass rdf:resource="#Theatre"/>
        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#hasTimeSlot"/>
        <owl:onClass rdf:resource="#TimeSlot"/>
        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <!-- ===================== -->
  <!-- Surgeon Individuals -->
  <!-- ===================== -->

  <owl:NamedIndividual rdf:about="#Dr_Silva">
    <rdf:type rdf:resource="#Surgeon"/>
    <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Dr Silva</hasName>
    <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mr Silva</hasName>
    <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Silva</hasName>
    <isPresent rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</isPresent>
    <maxDailyHours rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</maxDailyHours>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:about="#Dr_Peres">
    <rdf:type rdf:resource="#Surgeon"/>
    <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Dr Peres</hasName>
    <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mr Peres</hasName>
    <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Peres</hasName>
    <isPresent rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</isPresent>
    <maxDailyHours rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">10</maxDailyHours>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:about="#Dr_Fernando">
    <rdf:type rdf:resource="#Surgeon"/>
    <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Dr Fernando</hasName>
    <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mr Fernando</hasName>
    <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Fernando</hasName>
    <isPresent rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</isPresent>
    <maxDailyHours rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</maxDailyHours>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:about="#Dr_Jayasinghe">
    <rdf:type rdf:resource="#Surgeon"/>
    <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Dr Jayasinghe</hasName>
    <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mr Jayasinghe</hasName>
    <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Jayasinghe</hasName>
    <isPresent rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</isPresent>
    <maxDailyHours rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</maxDailyHours>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:about="#Dr_Kumar">
    <rdf:type rdf:resource="#Surgeon"/>
    <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Dr Kumar</hasName>
    <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mr Kumar</hasName>
    <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Kumar</hasName>
    <isPresent rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</isPresent>
    <maxDailyHours rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</maxDailyHours>
  </owl:NamedIndividual>



  <!-- ===================== -->
  <!-- Theatre Individuals -->
  <!-- ===================== -->

  <owl:NamedIndividual rdf:about="#Theatre_A">
    <rdf:type rdf:resource="#Theatre"/>
    <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Theatre A</hasName>
    <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Room A</hasName>
    <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Operating Theatre A</hasName>
    <isClean rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</isClean>
    <isUnderMaintenance rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</isUnderMaintenance>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:about="#Theatre_B">
    <rdf:type rdf:resource="#Theatre"/>
    <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Theatre B</hasName>
    <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Room B</hasName>
    <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Operating Theatre B</hasName>
    <isClean rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</isClean>
    <isUnderMaintenance rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</isUnderMaintenance>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:about="#Theatre_C">
    <rdf:type rdf:resource="#Theatre"/>
    <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Theatre C</hasName>
    <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Room C</hasName>
    <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Operating Theatre C</hasName>
    <isClean rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</isClean>
    <isUnderMaintenance rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</isUnderMaintenance>
  </owl:NamedIndividual>


</rdf:RDF>
